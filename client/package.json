{
  "name": "@u3t/client",
  "version": "1.0.0",
  "description": "React UI for U3T",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development && node ./index.js",
    "serve": "http-server dist",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --fix",
    "dev": "vite",
    "build": "tsc -b && vite build"
  },
  "reactSnap": {
