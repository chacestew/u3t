  "keywords": [
    "u3t",
    "u3t-app",
    "ultimate tic-tac-toe",
    "ultimate",
    "tic-tac-toe",
    "tic tac toe"
  ],
  "author": "Chace Stewart",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
