{
  "name": "@u3t/server",
  "version": "1.0.0",
  "description": "Socket.IO API for U3T",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && tsx ./index.ts",
    "start": "NODE_ENV=production && node build/index.js",
    "build": "del-cli build && tsc",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --fix"
  },
  "keywords": [
    "u3t",
    "u3t-app",
    "ultimate tic-tac-toe",
    "ultimate",
    "tic-tac-toe",
    "tic tac toe"
  ],
  "author": "Chace Stewart",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "trustedDependencies": [
    "bcrypt"
  ],
  "dependencies": {
    "@sentry/node": "^6.8.0",
    "@socket.io/admin-ui": "^0.2.0",
    "@types/clone-deep": "^4.0.1",
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.4",
    "@types/nodemailer": "^6.4.2",
    "bcrypt": "^5.1.1",
    "clone-deep": "^4.0.1",
    "del-cli": "^3.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.8",
    "nanoid": "^3.3.4",
    "socket.io": "^4.1.2",
    "tsx": "^4.19.2",
    "typescript": "^4.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "cors": "^2.8.5",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "2.2.1"
