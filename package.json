{
  "name": "u3t",
  "version": "0.1.0",
  "description": "Ultimate Tic-Tac-Toe",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --watch ./src/server -r esm ./src/server/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --config webpack/config.prod.js",
    "serve": "cross-env NODE_ENV=production node index.js",
    "tsc": "ts-node-dev ./src/server/index.ts",
    "build-game": "tsc ./src/shared/_game.ts --outDir ./src/shared/compiled",
    "babel": "babel ./src/shared/_game.ts",
    "analyze": "cross-env NODE_ENV=production webpack --profile --json > stats.json --config webpack/config.prod.js && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "tic-tac-toe",
    "ultimate",
    "tic",
    "tac",
    "toe"
  ],
  "author": "Chace Stewart",
  "license": "ISC",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/clone-deep": "^4.0.1",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/express": "^4.17.3",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.1.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^5.1.0",
    "clone-deep": "^4.0.1",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.0",
    "express-no-favicons": "0.0.1",
    "express-static-gzip": "^1.1.3",
    "nanoid": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.6",
    "react-router-dom": "^5.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "typescript": "^3.7.4",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
